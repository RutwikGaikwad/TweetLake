{
  "version":2,
  "builds":[
    {
      "src":"TweetLake/wsgi.py",
      "use":"@vercel/python",
      "config": {"maxLambdaSize": "15mb", "runtime":"python3.11.2"}
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles_build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "TweetLake/wsgi.py"
    }
  ]
}